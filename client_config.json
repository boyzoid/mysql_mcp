{
  "mcpServers": {
    "mysql_mcp": {
      "command": "path/to/node",
      "args": [
        "/path/to/project/index.js"
      ],
      "env": {
        "MYSQL_DB_HOST": "127.0.0.1",
        "MYSQL_DB_PORT": "3306",
        "MYSQL_DB_USER": "root",
        "MYSQL_DB_PASSWORD": "password",
        "MYSQL_DB_NAME": "database_name"
      }
    }
  }
}